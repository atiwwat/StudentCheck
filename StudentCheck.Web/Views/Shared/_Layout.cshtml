<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - StudentCheck.Web</title>

    <environment include="Development">
        <!-- Boostrap -->
        <link rel="stylesheet" href="~/lib/AdminLTE/bower_components/bootstrap/dist/css/bootstrap.css" />
        <!-- AdminLTE App -->
        <link href="~/lib/AdminLTE/dist/css/AdminLTE.css" rel="stylesheet" />
        <!-- All Skin App -->
        <link href="~/lib/AdminLTE/dist/css/skins/_all-skins.css" rel="stylesheet" />
        <!-- Font Awesome -->
        <link href="~/lib/AdminLTE/bower_components/font-awesome/css/font-awesome.css" rel="stylesheet" />
        <!-- DataTables -->
        <link href="~/lib/AdminLTE/bower_components/datatables.net-bs/css/dataTables.bootstrap.css" rel="stylesheet" />
    </environment>
    <environment exclude="Development">
        <!-- Boostrap -->
        <link rel="stylesheet" href="~/lib/AdminLTE/bower_components/bootstrap/dist/css/bootstrap.min.css" />
        <!-- AdminLTE App -->
        <link href="~/lib/AdminLTE/dist/css/AdminLTE.min.css" rel="stylesheet" />
        <!-- All Skin App -->
        <link href="~/lib/AdminLTE/dist/css/skins/_all-skins.min.css" rel="stylesheet" />
        <!-- Font Awesome -->
        <link href="~/lib/AdminLTE/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
        <!-- DataTables -->
        <link href="~/lib/AdminLTE/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    </environment>
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">StudentCheck</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="row">
        <div class="col-12">
            <img src="~/image/SiamImage.jpg" style="max-width: 100%;" />
        </div>
    </div>
    <div class="row" style="background-color: #1e282c;">
        <div class="col-sm-2">
            <ul class="sidebar-menu " data-widget="tree" style="background-color: #1e282c;">
                <li class="header">MAIN NAVIGATION</li>
                <li class="active treeview menu-open">
                    <a asp-controller="ManageProfessor" asp-action="Index">
                        จัดการข้อมูลอาจารย์
                    </a>
                </li>
                <li class="treeview menu-open">
                    <a asp-controller="ManageProfessor" asp-action="Index">
                        จัดการข้อมูลนักศึกษา
                    </a>
                </li>
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-calendar"></i> <span>จัดการวิชา</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu" style="display: none;">
                        <li><a href="../index.html"><i class="fa fa-circle-o"></i>ลงทะเบียนเรียน</a></li>
                        <li><a href="../index2.html"><i class="fa fa-circle-o"></i>ลงวิชาสอน</a></li>
                    </ul>
                </li>
            </ul>
        </div>
        <div class="col-sm-10" style="background-color: #ffffff;">
            @RenderBody()
        </div>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2019 - StudentCheck.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <environment include="Development">
        <!-- Jquery -->
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <!-- Boostrap -->
        <script src="~/lib/AdminLTE/bower_components/bootstrap/dist/js/bootstrap.js"></script>
        <!-- Jquery Ajax Unobtrusive -->
        <script src="~/lib/jquery-ajax-unobtrusive/dist/jquery.unobtrusive-ajax.js"></script>
        <!-- AdminLTE App -->
        <script src="~/lib/AdminLTE/dist/js/adminlte.js"></script>
        <!-- DataTables -->
        <script src="~/lib/AdminLTE/bower_components/datatables.net/js/jquery.dataTables.js"></script>
        <script src="~/lib/AdminLTE/bower_components/datatables.net-bs/js/dataTables.bootstrap.js"></script>
        <!-- Jquery Validation -->
        <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    </environment>
    <environment exclude="Development">
        <!-- Jquery -->
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <!-- Boostrap -->
        <script src="~/lib/AdminLTE/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
        <!-- Jquery Ajax Unobtrusive -->
        <script src="~/lib/jquery-ajax-unobtrusive/dist/jquery.unobtrusive-ajax.min.js"></script>
        <!-- AdminLTE App -->
        <script src="~/lib/AdminLTE/dist/js/adminlte.min.js"></script>
        <!-- DataTables -->
        <script src="~/lib/AdminLTE/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
        <script src="~/lib/AdminLTE/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
        <!-- Jquery Validation -->
        <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    </environment>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
