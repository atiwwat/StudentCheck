<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - StudentCheck.Web</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/AdminLTE/bower_components/bootstrap/dist/css/bootstrap.css" />

        <!-- AdminLTE App -->
        <link href="~/lib/AdminLTE/dist/css/AdminLTE.min.css" rel="stylesheet" />

        <!-- All Skin App -->
        <link href="~/lib/AdminLTE/dist/css/skins/_all-skins.min.css" rel="stylesheet" />

        <!-- Font Awesome -->
        <link href="~/lib/AdminLTE/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

        <!-- DataTables -->
        <link href="~/lib/AdminLTE/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css"
              asp-fallback-href="~/lib/AdminLTE/bower_components/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute"
              crossorigin="anonymous"
              integrity="sha256-eSi1q2PG6J7g7ib17yAaWMcrr5GrtohYChqibrV7PBE=" />
    </environment>
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">StudentCheck</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="row">
        <div class="col-12">
            <img src="~/image/SiamImage.jpg" class="mw-100" />
        </div>
    </div>
    <div class="row">
        <div class="col-sm-2 skin-blue sidebar-mini">
            <ul class="sidebar-menu " data-widget="tree" style="background-color: #1e282c;">
                <li class="header">MAIN NAVIGATION</li>
                <li class="active treeview menu-open">
                    <a asp-controller="ManageProfessor" asp-action="Index">
                        จักการข้อมูลอาจารย์
                    </a>
                </li>
            </ul>
        </div>
        <div class="col-sm-9">
            @RenderBody()
        </div>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2019 - StudentCheck.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        @*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>*@
        <script src="~/lib/AdminLTE/bower_components/bootstrap/dist/js/bootstrap.js"></script>

        <!-- Jquery Ajax Unobtrusive -->
        <script src="~/lib/jquery-ajax-unobtrusive/dist/jquery.unobtrusive-ajax.js"></script>

        <!-- AdminLTE App -->
        <script src="~/lib/AdminLTE/dist/js/adminlte.js"></script>

        <!-- DataTables -->
        <script src="~/lib/AdminLTE/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
        <script src="~/lib/AdminLTE/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=">
        </script>
        @*<script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/js/bootstrap.bundle.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha256-E/V4cWE4qvAeO5MOhjtGtqDzPndRO1LBk8lJ/PR7CA4=">
        </script>*@

        <script src="~/lib/AdminLTE/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

        <!-- Jquery Ajax Unobtrusive -->
        <script src="~/lib/jquery-ajax-unobtrusive/dist/jquery.unobtrusive-ajax.min.js"></script>

        <!-- AdminLTE App -->
        <script src="~/lib/AdminLTE/dist/js/adminlte.min.js"></script>
    </environment>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
